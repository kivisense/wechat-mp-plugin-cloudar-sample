canvas{
  width: 100%;
  height: 100%;
}
.button {
  margin-top: 10rpx;
  background: #000000;
  padding: 8rpx;
  width: 170rpx;
  color: #ffffff;
}
.text {
  white-space: pre-wrap;
}
view {
  margin-top: 20rpx;
  text-align: center;
}
image {
  display: block;
  margin: 20rpx auto;
}
.layer {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}
.ui{
  z-index:2;
}
.gift {
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  width: 500rpx;
  height: 500rpx;
  z-index:999;
}